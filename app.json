{
  "expo": {
    "name": "EventSphere India",
    "slug": "eventsphere-india",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.eventsphereindia",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.yourname.eventsphereindia",
            "CFBundleURLSchemes": [
              "msauth.com.yourname.eventsphereindia"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.yourname.eventsphereindia",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "msauth",
              "host": "com.yourname.eventsphereindia",
              "pathPrefix": "/callback"
            }
          ],
          "category": [
            "DEFAULT",
            "BROWSABLE"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-location",
      "expo-image-picker",
      "expo-notifications",
      "expo-font"
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "experiments": {
      "reactCanary": false
    },
    "extra": {
      "eas": {
        "projectId": "aa6e66e6-9123-4422-bdbd-8a4c1b5f3cfb"
      }
    }
  }
}

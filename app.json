{
  "expo": {
    "name": "EventSphere India",
    "slug": "eventsphere-india",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.eventsphereindia",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.yourname.eventsphereindia",
            "CFBundleURLSchemes": ["msauth.com.yourname.eventsphereindia"]
          }
        ]
      }
    },
    "android": {
      "package": "com.yourname.eventsphereindia",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "msauth",
              "host": "com.yourname.eventsphereindia",
              "pathPrefix": "/callback"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        }
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-location", 
      "expo-image-picker",
      "expo-notifications",
      "expo-font"
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "experiments": {
      "reactCanary": false
    }
  }
}
